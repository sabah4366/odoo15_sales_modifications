<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!--sequence for the sales_task model-->
        <record id="classroom_sequence_data" model="ir.sequence">
            <field name="name">Sales_task.project.estimation</field>
            <field name="code">sales_task.project.estimation</field>
            <field name="prefix">0</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--sequence for the job_order model-->
        <record id="job_order_sequence_data" model="ir.sequence">
            <field name="name">Sales_task.job.order</field>
            <field name="code">sales_task.job.order</field>
            <field name="prefix">J0</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>

</odoo>